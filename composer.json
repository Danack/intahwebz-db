{
    "name": "intahwebz/db",
    "description": "Library for using a database as a database - not ORM, just straight DB abstraction.",
    "keywords": ["intahwebz", "core", "library", "db", "database"],

    "autoload": {
        "psr-0": {
            "Intahwebz\\DB": "src/",
            "Intahwebz\\DBSync": "src/",
            "Intahwebz\\TableMap": "src/"
        }
    },

    "license": "MIT",

    "authors": [
        {
            "name": "Dan Ackroyd",
            "email": "Danack@basereality.com",
            "homepage": "http://www.basereality.com"
        }
    ],

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Danack/Auryn"
        }
    ],

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "monolog/monolog": "1.4.0",
        "rdlowrey/auryn": "dev-danackExceptionRefactor as v0.8.1-dev",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
    },

    "require":{
        "intahwebz/mb_extra": "dev-master",
        "php": ">=5.4",
        "psr/log": "~1.0",
        "intahwebz/core": "dev-master"
    }
}